<ul>
  <% @users.each do |user| %>
    <% if user.first_name != nil %>
    <div class="user-tile">
        <% fullname = user.first_name+" "+user.last_name %>
        <li><%= link_to fullname, user %></li>
        <li><%= user.interests %></li>
        <li><%= user.status %></li>
    <% else %>
       <li><%= user.email %> has not filled out their profile yet</li>
    <% end %>
    </div>
    <br>
  <% end %>
</ul>